@model IEnumerable<AplicacaoMedicina.Models.MedicoConsultorio>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutMedico.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Consultorio.Nome_Consu)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Medico.Nome_Medi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ativo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Consultorio.Nome_Consu)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Medico.Nome_Medi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ativo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_MediConsu }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID_MediConsu }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID_MediConsu })
        </td>
    </tr>
}

</table>
