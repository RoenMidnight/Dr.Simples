@model IEnumerable<AplicacaoMedicina.Models.MedicoArea>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Area.Nome_Area)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Medico.Nome_Medi)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Area.Nome_Area)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Medico.Nome_Medi)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_MediArea }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID_MediArea }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID_MediArea })
        </td>
    </tr>
}

</table>
